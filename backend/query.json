"{\"SelectBenutzer\": \"SELECT * FROM benutzer;\", \"InsertBenutzer\": \"INSERT INTO benutzer (Anmeldename, Passwort, Telefonnummer, Administrator) VALUES ('{p1}', '{p2}', '{p3}', {p4});\", \"UpdateBenutzer\": \"UPDATE benutzer SET Telefonnummer = '{p4}', Administrator = {p5}, Passwort = '{p3}' WHERE BenutzerID = {p1};\", \"DeleteBenutzer\": \"DELETE FROM benutzer WHERE BenutzerID = {p1};\", \"SelectSensor\": \"SELECT * FROM sensor;\", \"InsertSensor\": \"INSERT INTO sensor (Serverschrank, HerstellerID, MaximalTemperatur, Adresse) VALUES ({p2}, {p3}, {p4}, '{p5}');\", \"UpdateSensor\": \"UPDATE sensor SET Serverschrank = {p2}, HerstellerID = {p3}, MaximalTemperatur = {p4}, Adresse = '{p5}' WHERE SensorID = {p1};\", \"DeleteSensor\": \"DELETE FROM sensor WHERE SensorID = {p1};\", \"SelectTemperatur\": \"SELECT * FROM temperatur;\", \"InsertTemperatur\": \"INSERT INTO temperatur (SensorID, Temperatur) VALUES ({p3}, {p4});\", \"UpdateTemperatur\": \"UPDATE temperatur SET SensorID = {p3}, Temperatur = {p4} WHERE TemperaturID = {p1};\", \"DeleteTemperatur\": \"DELETE FROM temperatur WHERE TemperaturID = {p1};\", \"SelectLog\": \"SELECT * FROM log;\", \"InsertLog\": \"INSERT INTO log (SensorID, BenutzerID, MaximalTemperatur) VALUES ({p1}, {p3}, {p4});\", \"UpdateLog\": \"UPDATE log SET SensorID = {p3}, BenutzerID = {p4}, MaximalTemperatur = {p5} WHERE LogID = {p1}\", \"DeleteLog\": \"DELETE FROM log WHERE LogID = {p1};\", \"SelectHersteller\": \"SELECT * FROM hersteller;\", \"InsertHersteller\": \"INSERT INTO hersteller (Name) VALUES ('{p2}');\", \"UpdateHersteller\": \"UPDATE hersteller SET Name = '{p2}' WHERE HerstellerID = {p1}\", \"DeleteHersteller\": \"DELETE FROM hersteller WHERE HerstellerID = {p1};\", \"6bS\": \"SELECT sensor.SensorID, sensor.Serverschrank, sensor.MaximalTemperatur, sensor.Adresse, hersteller.HerstellerID, hersteller.Name FROM sensor JOIN hersteller ON sensor.HerstellerID = hersteller.HerstellerID;\", \"6cS\": \"SELECT sensor.SensorID, sensor.Serverschrank, sensor.MaximalTemperatur, sensor.Adresse, sensor.HerstellerID, temperatur.TemperaturID, temperatur.Zeit, temperatur.Temperatur FROM sensor LEFT JOIN temperatur ON sensor.SensorID = temperatur.SensorID;\", \"6dS\": \"SELECT temperatur.TemperaturID, temperatur.Zeit, temperatur.Temperatur, sensor.SensorID, sensor.Serverschrank, sensor.MaximalTemperatur, sensor.Adresse, hersteller.HerstellerID, hersteller.Name FROM temperatur LEFT JOIN sensor ON sensor.SensorID = temperatur.SensorID LEFT JOIN hersteller ON sensor.HerstellerID = Hersteller.HerstellerID;\", \"6eS\": \"SELECT * FROM temperatur WHERE TemperaturID > (SELECT COUNT(*) FROM temperatur) - 10;\", \"6fS\": \"SELECT * FROM temperatur ORDER BY TemperaturID DESC LIMIT 10;\", \"6gS\": \"SELECT sensor.SensorID, sensor.Serverschrank, sensor.MaximalTemperatur, sensor.Adresse, sensor.HerstellerID, temperatur.TemperaturID, temperatur.Zeit, MAX(temperatur.Temperatur) FROM sensor LEFT JOIN temperatur ON sensor.SensorID = temperatur.SensorID GROUP BY sensor.SensorID;\", \"6hS\": \"SELECT sensor.SensorID, sensor.Serverschrank, sensor.MaximalTemperatur, sensor.Adresse, sensor.HerstellerID, temperatur.TemperaturID, temperatur.Zeit, AVG(temperatur.Temperatur) FROM sensor LEFT JOIN temperatur ON sensor.SensorID = temperatur.SensorID GROUP BY sensor.SensorID;\", \"7cS\": \"SELECT SensorID, MaximalTemperatur FROM sensor;\", \"7cU\": \"UPDATE sensor SET MaximalTemperatur = {p2} WHERE SensorID = {p1};\", \"7cS2\": \"SELECT MaximalTemperatur FROM log WHERE SensorID = {p1} ORDER BY Zeit DESC;\", \"LIS1\": \"SELECT * FROM benutzer WHERE Anmeldename = '{p1}' AND Passwort = '{p2}';\", \"LIS2\": \"SELECT * FROM benutzer WHERE Anmeldename = '{p1}';\", \"LIS3\": \"SELECT * FROM benutzer WHERE Anmeldename = '{p1}' AND Admin = 1;\", \"IAS\": \"SELECT Administrator FROM benutzer WHERE Anmeldename = '{p1}';\"}"